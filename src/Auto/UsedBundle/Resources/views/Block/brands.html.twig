{% if items|length > 0 %}
<div class="row">
    {% for item in items %}
        <div class="col-sm-2" style="min-height: 35px">
            <a href="{{ path('pages_automobile', {brand_alias: item.alias}) }}">
                {% if item.logo %}
                    <img src="{{ item.logo.image.path }}" width="30px">
                {% endif %}
                {{ item.name }}
            </a>
        </div>
    {% endfor %}
</div>
{% endif %}