<div class="form-group">
    <label for="city" class="col-sm-4 control-label">Где находится автомобиль?</label>
    <div class="col-sm-4">
        <select name="location" id="location">
        </select>
    </div>
</div>
<div class="form-group">
    <label for="name" class="control-label col-sm-4">Ваше имя</label>
    <div class="col-sm-4">
        <input type="text" class="form-control input-sm" id="name" name="name" placeholder="" value="{% if is_granted('ROLE_USER') %}{{ app.user.username }}{% endif %}">
    </div>
</div>
<div class="form-group">
    <label for="name" class="col-sm-4 control-label">Номер телефона</label>
    <div class="col-sm-4">
        <input type="text" name="phone" class="form-control input-sm" id="phone" placeholder="+375292247887" value="">
    </div>
</div>
<div class="form-group">
    <label for="name" class="col-sm-4 control-label">Дополнительный номер телефона</label>
    <div class="col-sm-4">
        <input type="text" name="phone_1" class="form-control input-sm" id="phone_1" placeholder="" value="">
    </div>
</div>